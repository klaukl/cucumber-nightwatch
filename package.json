{
  "name": "cucumber-nightwatch",
  "version": "0.1.0",
  "description": "Example for using Nightwatch.js with cucumber-js CLI",
  "scripts": {
    "test": "node ./node_modules/@cucumber/cucumber/bin/cucumber-js features/**/*.feature --require features/**/*.ts --require-module ts-node/register  --format html:report.html --format @cucumber/pretty-formatter"
  },
  "author": "Tim Yao",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "chromedriver": "^98.0.0",
    "geckodriver": "^3.0.1",
    "nightwatch": "^2.0.7"
  },
  "devDependencies": {
    "@types/node": "^17.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
